<Window x:Class="KnightsDatabaseInterface.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KnightsDatabaseInterface"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TabPanel>
            <TabItem MinWidth="50" Header="Knights">
                <local:KnightsView />
            </TabItem>
            <TabItem MinWidth="50" Header="Councils"/>
            <TabItem MinWidth="50" Header="State Leadership" />
        </TabPanel>
    </Grid>
</Window>
